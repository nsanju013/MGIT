							<div class="uilogin_module" ><script language="javascript" type="text/javascript" src="/templates/rhuk_milkyway/js/captchascript/js/captcha_checker.js"></script>
<script type="text/javascript">
    jQuery(document).ready(function()
    {	
        jQuery('#username').focus();
		
    });
	
	//
	function checkCaptcha()
	{
	    var vpb_captcha_code = jQuery("#captcha").val();
	    var passwd=jQuery("#modlgn_passwd").val();
	    var username=jQuery("#username").val();
		var checkcaptcha=parseInt(jQuery('#checkcaptcha').val());
	    if (username=='' || passwd=='' || (vpb_captcha_code=='' && checkcaptcha==1))
		{
			if(username=='')
			{		
				jQuery("#usernameerr").html('Username is required.');		    
			}
			else
			{
				jQuery("#usernameerr").html('');
			}
			
			if (passwd==''){			    
				jQuery("#passworderr").html('Password is required.');
			}
			else
			{
				jQuery("#passworderr").html('');
			}
			
			if(vpb_captcha_code=='')
			{
				jQuery('#captchaerr').html('Captcha is required.');
			}
			else
			{
				jQuery("#captchaerr").html('');
			}
			return false;
	    }
	    else
	    {
			jQuery("#usernameerr").html('');
			jQuery("#passworderr").html('');
			jQuery("#captchaerr").html('');
			
			if(checkcaptcha==1){
				var dataString = 'captcha='+ vpb_captcha_code;
				$.ajax(
				{
					type: "POST",
					url: "/templates/rhuk_milkyway/js/captchascript/captcha_checker.php",
					data: dataString,
					cache: false,
					beforeSend: function() 
					{
						jQuery("#captchaerr").html('<font style="font-family:Lato; font-size:12px; color:black;">Please wait</font>');
					},
					success: function(response)
					{
						if (response==0)
						{
							jQuery("#captchaerr").hide().fadeIn('slow').html('');
							$('#form-login').submit();
							return true;
						}
						else
						{
							vpb_refresh_aptcha(); //Refresh the Captcha Image on success or on wrong security submitted
							jQuery("#captcha").val('');
							jQuery("#captchaerr").hide().fadeIn('slow').html('Invalid captcha. Please try again.');
							return false;
						}
					}
				});
			}
			else{
				$('#form-login').submit();
				return true;
			}
			return false;
	    }
	}
</script>

	    		
